<% provide(:title, 'All users') %>
      <h1>All users</h1>
      <div class="row">
        <div class="span6 offset3">
          <% if can? :create, User %>
          <p class="center">
            <%= link_to "Create user", new_user_path, class: "btn btn-primary" %>
          </p>
          <% end %>
          <table class="table table-striped">
          <% @users.each do |user| %>
<%= render user %>
          <% end %>
          </table>
          <%= will_paginate %>
        </div>
      </div>
